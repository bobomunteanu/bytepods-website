{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      },
      {
        "src": "hugo.exe",
        "use": "@vercel/hugo",
        "config": {
          "version": "0.74.0"
        }
      }
    ],
    "routes": [
      {
        "src": ".*",
        "dest": "/public"
      }
    ]
  }